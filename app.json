{
  "expo": {
    "name": "metacritic-app",
    "slug": "metacritic-app",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "metacritic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#111111"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.abrahamgalue.metacriticapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "91eca20b-5915-4d59-b546-59d225afbb39"
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#00000000"
    },
    "plugins": [
      ["expo-router"],
      ["./utils/transforms/withAndroidSplashScreen.ts"]
    ]
  }
}
